<% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
<div
  id="alert"
  class="alert alert-warning alert-dismissible fade show"
  role="alert"
>
  <%= error.msg %>
  <button
    onclick="setTimeout(function () {document.getElementById('alert').style.display='none'}, 50); return false"
    type="button"
    class="close"
    data-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %> <% } %> <% if(success_msg != ''){ %>
<div
  id="alert"
  class="alert alert-success alert-dismissible fade show"
  role="alert"
>
  <%= success_msg %>
  <button
    onclick="setTimeout(function () {document.getElementById('alert').style.display='none'}, 50); return false"
    type="button"
    class="close"
    data-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error_msg != ''){ %>
<div
  id="alert"
  class="alert alert-danger alert-dismissible fade show"
  role="alert"
>
  <%= error_msg %>
  <button
    onclick="setTimeout(function () {document.getElementById('alert').style.display='none'}, 50); return false"
    type="button"
    class="close"
    data-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error != ''){ %>
<div
  id="alert"
  class="alert alert-danger alert-dismissible fade show"
  role="alert"
>
  <%= error %>
  <button
    onclick="setTimeout(function () {document.getElementById('alert').style.display='none'}, 50); return false"
    type="button"
    class="close"
    data-dismiss="alert"
    aria-label="Close"
  >
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
